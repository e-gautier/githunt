{
  "name": "githunt",
  "version": "1.4.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@redux-devtools/extension": "^3.2.3",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.2.2",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-helmet-async": "^1.3.0",
    "react-infinite-scroller": "^1.2.6",
    "react-redux": "^7.0",
    "react-responsive-modal": "^6.2.0",
    "react-scroll-up": "^1.3.7",
    "react-tooltip": "^4.4.0",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.3.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.55.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --print-width 120 --single-quote --write"
    ]
  },
  "scripts": {
    "flow": "flow",
    "lint": "prettier --print-width 120 --single-quote --write src/**/*.{js,jsx,json,scss}",
    "precommit": "lint-staged",
    "start": "npm-run-all -p watch-css start-js",
    "build": "rm -rf build; npm-run-all build-css build-js && cp public/manifest.json build",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build && cp src/assets/img/icon* build/static/media",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "sass src/:src/",
    "watch-css": "npm run build-css && sass src/:src/ --watch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "@redux-devtools/dock-monitor": "^3.0.1",
    "@redux-devtools/log-monitor": "^4.0.1",
    "flow-bin": "^0.190.0",
    "react-scripts": "^5.0.1"
  }
}
